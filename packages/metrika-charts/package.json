{
   "name": "@metrika/metrika-charts",
   "version": "0.0.2",
   "license": "Apache-2.0",
   "main": "cjs/index.js",
   "module": "build/index.js",
   "types": "build/index.d.ts",
   "scripts": {
      "build:clean": "rimraf build cjs",
      "build:css": "sass ./src/metrika-theme.scss ./build/metrika-theme.css",
      "build:es5": "tsc -p tsconfig.es5.json",
      "build:es6": "tsc -p tsconfig.json",
      "build": "npm run build:clean && npm run build:es6 && npm run build:es5 && npm run build:css",
      "start": "npm run build:css && concurrently --names 'tsc,eslint' -c 'bgMagenta,bgBlue' 'tsc --watch' 'pnpm run lint'",
      "lint": "esw -w --ext .ts,.tsx ./src",
      "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
      "publish": "npm publish --access public"
   },
   "sideEffects": false,
   "dependencies": {
      "@metrika/elastic-charts": "^31.7.2",
      "@nivo/core": "^0.79.0",
      "@nivo/line": "^0.79.1",
      "@nivo/treemap": "^0.79.1",
      "@nivo/tooltip": "^0.79.0",
      "d3-format": "^3.1.0",
      "deepmerge": "^4.2.2",
      "dayjs": "^1.11.4",
      "prop-types": "^15.8.1"
   },
   "peerDependencies": {
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
   },
   "devDependencies": {
      "@babel/core": "^7.18.9",
      "@babel/preset-env" : "^7.18.9",
      "@babel/preset-typescript": "^7.18.6",
      "@types/d3-format": "^3.0.1",
      "@types/jest": "^28.1.6",
      "@types/node": "^18.6.2",
      "@types/react": "^17.0.48",
      "@types/react-dom": "^17.0.17",
      "@jest/types": "^28.1.3",
      "babel-jest": "^28.1.3",
      "concurrently": "^7.3.0",
      "cross-env": "^7.0.3",
      "jest": "^28.1.3",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "rimraf": "^3.0.2",
      "sass": "^1.54.0",
      "ts-node": "^10.9.1",
      "typescript": "^4.7.4"
   }
}
