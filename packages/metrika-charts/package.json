{
   "name": "@metrika/metrika-charts",
   "version": "0.0.19",
   "license": "Apache-2.0",
   "main": "cjs/index.js",
   "module": "build/index.js",
   "types": "build/index.d.ts",
   "scripts": {
      "build:clean": "rimraf build cjs",
      "build:css": "sass ./src/metrika-theme.scss ./build/metrika-theme.css",
      "build:es5": "tsc -p tsconfig.es5.json",
      "build:es6": "tsc -p tsconfig.json",
      "build": "npm run build:clean && npm run build:es6 && npm run build:es5 && npm run build:css",
      "start": "npm run build:css && concurrently --names 'tsc,eslint' -c 'bgMagenta,bgBlue' 'tsc --watch' 'pnpm run lint'",
      "lint": "esw -w --ext .ts,.tsx ./src",
      "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
      "publish": "npm publish --access public"
   },
   "sideEffects": false,
   "dependencies": {
      "@metrika/elastic-charts": "^31.7.5",
      "@nivo/core": "^0.80.0",
      "@nivo/line": "^0.80.0",
      "@nivo/treemap": "^0.80.0",
      "@nivo/tooltip": "^0.80.0",
      "echarts-for-react": "^3.0.2",
      "echarts": "^5.4.3",
      "d3-format": "^3.1.0",
      "deepmerge": "^4.3.1",
      "dayjs": "^1.11.7",
      "prop-types": "^15.8.1"
   },
   "peerDependencies": {
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
   },
   "devDependencies": {
      "@babel/core": "^7.21.4",
      "@babel/preset-env" : "^7.21.4",
      "@babel/preset-typescript": "^7.21.4",
      "@types/d3-format": "^3.0.1",
      "@types/jest": "^29.5.0",
      "@types/node": "^18.15.11",
      "@types/react": "^17.0.58",
      "@types/react-dom": "^17.0.19",
      "@jest/types": "^29.5.0",
      "babel-jest": "^29.5.0",
      "concurrently": "^8.0.1",
      "cross-env": "^7.0.3",
      "jest": "^29.5.0",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "rimraf": "^5.0.0",
      "sass": "^1.62.0",
      "ts-node": "^10.9.1",
      "typescript": "^5.0.4"
   }
}
